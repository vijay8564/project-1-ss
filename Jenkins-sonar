pipeline{
    agent {
        label 'agent'
    }
    stages{
        stage('test'){
            steps{
                git 'https://github.com/vijaykumar8564/secretsanta-generator.git'
            }
        }
        stage('build'){
            steps{
                sh "mvn clean verify sonar:sonar \
  -Dsonar.projectKey=secret-santa"
            }
        }
    }
}
